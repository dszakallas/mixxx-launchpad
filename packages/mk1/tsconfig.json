{
  "compilerOptions": {
    "composite": true,
    "isolatedModules": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Node",
    "outDir": "dist",
    "resolveJsonModule": true
  },
  "references": [{ "path": "../mixxx" }, { "path": "../app" }],
  "include": ["controller.json", "src"]
}
